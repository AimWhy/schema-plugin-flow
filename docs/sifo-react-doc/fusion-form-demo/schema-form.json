{
  "component": "Container",
  "id": "main_id",
  "attributes":{
    "className": "fusion-form-demo-main"
  },
  "children": [
    {
      "id": "projectId",
      "component": "Input",
      "attributes": {
        "className": "test-projectId",
        "name": "projectId",
        "label": "项目名称",
        "defaultValue": "d",
        "placeholder": "项目名称，包含校验规则与校验函数",
        "rules": [
          {
            "required": true,
            "message": "你需要填写项目的名称",
            "trigger": [
              "onPressEnter",
              "onChange"
            ]
          }
        ]
      }
    },
    {
      "component": "Select",
      "attributes": {
        "id": "type",
        "name": "type",
        "label": "类型",
        "placeholder": "请选择类型",
        "style": {
          "width": "100%"
        },
        "rules": [
          {
            "required": true
          }
        ]
      }
    },
    {
      "component": "Input",
      "id": "typeDesc",
      "attributes": {
        "name": "typeDesc",
        "label": "类型描述",
        "placeholder": "类型描述",
        "rules": [
          {
            "required": true
          }
        ]
      }
    },
    {
      "component": "DatePicker",
      "id": "date",
      "attributes": {
        "name": "date",
        "showTime": false,
        "label": "日期",
        "placeholder": "日期",
        "rules": [
          {
            "required": true
          }
        ]
      }
    },
    {
      "component": "DatePicker",
      "id": "time",
      "attributes": {
        "name": "time",
        "showTime": true,
        "label": "日期时间",
        "placeholder": "日期时间",
        "rules": [
          {
            "required": true
          }
        ]
      }
    },
    {
      "component": "Input",
      "id": "subject",
      "attributes": {
        "className": "test-subject",
        "itemClassName": "subject-form-item",
        "name": "subject",
        "label": "设置字段样式",
        "placeholder": "请输入主题",
        "labelCol": {
          "span": 8
        },
        "wrapperCol": {
          "span": 8,
          "offset": 0
        }
      }
    },
    {
      "component": "Container",
      "id": "sub_contain",
      "children": [
        {
          "component": "Input",
          "attributes": {
            "id": "numbertest",
            "name": "numbertest",
            "label": "整数校验",
            "rules": [
              {
                "type": "integer"
              }
            ]
          }
        },
        {
          "component": "Input",
          "attributes": {
            "id": "floattest",
            "name": "floattest",
            "label": "最大值校验",
            "rules": [
              {
                "max": 99.9
              }
            ]
          }
        },
        {
          "component": "Input",
          "attributes": {
            "id": "lentest",
            "name": "lentest",
            "label": "仅提交时长度校验",
            "rules": [
              {
                "maxLength": 9,
                "notAutoTrigger": true
              }
            ]
          }
        },
        {
          "component": "Input",
          "attributes": {
            "id": "tt",
            "name": "tt",
            "label": "无校验"
          }
        },
        {
          "id": "innerComp",
          "component": "Container",
          "attributes": {
            "className": "test-projectId",
            "name": "innerComp",
            "label": "内层组件测试"
          },
          "children": [
            "测试字段内层组件"
          ]
        }
      ]
    },
    {
      "component": "Container",
      "id": "footerBar",
      "children": [
        {
          "component": "Button",
          "attributes": {
            "id": "submitBtn",
            "type": "primary",
            "title": "提交"
          }
        },
        {
          "component": "Button",
          "attributes": {
            "id": "resetBtn",
            "type": "normal",
            "title": "设置数据"
          }
        }
      ],
      "attributes": {
        "className": "footer-opt"
      }
    }
  ]
}